@model IEnumerable<PlanyATH_Project.Models.DataModelView>

<div id="result"></div>
<div>
    <div id="personlist-section">
        <table class="table table-bordered" style="background-color: lightgray">
            <tr>
                <th>
                    Nazwa
                </th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @using (Ajax.BeginForm("ICSReader", "Home", new AjaxOptions() { UpdateTargetId = "costam" }))
                        {
                            <input type="submit" class="btn btn-default btn-block" name="path" value="@item.Name" />
                        }
                    </td>
                </tr>
            }
        </table>
        <div id="olaboga">

        </div>
    </div>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="~/scripts/jquery.unobtrusive-ajax.js"></script>
